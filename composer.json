{
    "name": "anysite-dev/filament-frontend-builder",
    "description": "Block-Based Frontend Builder for Filament",
    "keywords": [
        "anysite",
        "laravel",
        "filament",
        "frontend",
        "blocks"
    ],
    "homepage": "https://github.com/anysite-dev/filament-frontend-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "anysite.dev",
            "email": "support@anysite.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^9.0 | ^10.0 | ^11.0",
        "spatie/laravel-package-tools": "^1.13.5"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0 | ^8.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "AnysiteDev\\FilamentFrontendBuilder\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnysiteDev\\FilamentFrontendBuilder\\FilamentFrontendBuilderServiceProvider"
            ],
            "aliases": {
                "FilamentFrontendBuilder": "AnysiteDev\\FilamentFrontendBuilder\\Facades\\FilamentFrontendBuilder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
